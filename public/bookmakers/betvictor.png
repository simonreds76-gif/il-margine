<script>
  (function getAllUrlParams(url) {
    function getStorage() {
      var storage;
      try {
        storage = window.localStorage;  
        storage.setItem('testSessionStorageKey', 'test value');
        storage.getItem('testSessionStorageKey');
        storage.removeItem('testSessionStorageKey');
      } catch (e) {
        window.temporaryStorage = {};
        storage = {
          getItem: function (key) {return (key in window.temporaryStorage) ? window.temporaryStorage[key] : null},
          setItem: function (key, value) { window.temporaryStorage[key] = value },
        };  
      }
      return storage;
    };
    function attributionScore(attrObject) {
      var attrKeys = attrObject ? Object.keys(attrObject) : null;
      var baseScore = attrKeys ? attrKeys.length : 0;
      var isAffid = attrKeys && attrKeys.includes('affid') && !attrKeys.includes('referrer') ? 1 : 0;
      return parseInt(baseScore + isAffid);
    }
    function rewriteAttribution(storedAttr, newAttr) {
      var storedAttr = JSON.parse(storedAttr);
      var newAttr = JSON.parse(newAttr);
      var storedScore = attributionScore(storedAttr);
      var newScore = attributionScore(newAttr);
  
      if (!storedAttr || !storedAttr.ts || (storedAttr.ts < (Date.now() - (3600 * 24 * 7)))) {
        return true;
      } else if (
        (newAttr.referrer && decodeURIComponent(newAttr.referrer).startsWith('https://' + document.location.hostname)) ||
        (document.referrer && document.referrer.startsWith('https://' + document.location.hostname)) || 
        (decodeURIComponent(newAttr.referrer).indexOf('thunderbite.com') > -1) ||
        newScore < storedScore
      ) {
        return false;
      } 
      return true;
    }
  
    var queryString = url ? url.split('?')[1] : window.location.search.slice(1);
    var obj = {};
  
    if (queryString) {
      queryString = queryString.split('#')[0];
      var arr = queryString.split('&');
  
      arr.forEach(function (param) {
        var [paramName, paramValue] = param.split('=');
        paramValue = typeof (paramValue) === 'undefined' ? true : paramValue;
  
        if (paramName.match(/\[(\d+)?\]$/)) {
          var key = paramName.replace(/\[(\d+)?\]/, '');
          if (!obj[key]) obj[key] = [];
          if (paramName.match(/\[\d+\]$/)) {
            var index = /\[(\d+)\]/.exec(paramName)[1];
            obj[key][index] = paramValue;
          } else {
            obj[key].push(paramValue);
          }
        } else {
          if (!obj[paramName]) {
            obj[paramName] = paramValue;
          } else if (obj[paramName] && typeof obj[paramName] === 'string') {
            obj[paramName] = [obj[paramName], paramValue];
          } else {
            obj[paramName].push(paramValue);
          }
        }
      });
    }
    obj.ts = Date.now();
    obj.landing_page = location.pathname;
    obj.hostname = document.location.hostname;
    if (document.referrer) {
      obj.referrer = document.referrer;
    }
  
    var regex = `^((?!(${ 'tblci|ScCid|rdt_cid|clickid|bca_ID|sub1|aclid|aid|click_id|c|campaign_id|twclid|utm_keyword|term|adgroupid|ad_id|ad|raf|reclid|pid|utm_medium|fbclid|language|mopinion|referrer|utm_content|utm_term|mid|gclid|btagid|nid|crmid|utm_source|utm_campaign|adgroup|btagc|btagb|btaga|btag|affiliateid|affid' ? 'tblci|ScCid|rdt_cid|clickid|bca_ID|sub1|aclid|aid|click_id|c|campaign_id|twclid|utm_keyword|term|adgroupid|ad_id|ad|raf|reclid|pid|utm_medium|fbclid|language|mopinion|referrer|utm_content|utm_term|mid|gclid|btagid|nid|crmid|utm_source|utm_campaign|adgroup|btagc|btagb|btaga|btag|affiliateid|affid' + '|' : '' }hostname|landing_page|referrer|ts)).)*$`;
    Object.keys(obj).forEach(function (key) {
      if (key.match(regex)) {
        delete obj[key];
      }
    });
  
    var storage = getStorage();
    var additional_mapping_params = JSON.parse('{"affiliateid":"acquisitionid","affid":"acquisitionid"}');
    if (obj['btag']) {
      var btag = obj['btag'];
      if (btag) {
        Object.assign(obj, {
          btaga: !obj.btaga ? btag.split('a_')[1]?.split('b_')[0] : null,
          btagb: !obj.btagb ? btag.split('b_')[1]?.split('c_')[0] : obj.btagb,
          btagc: !obj.btagc ? btag.split('c_')[1] : null
        });
      }
    }
    Object.keys(additional_mapping_params).forEach(sourceKey => { 
      if (obj.hasOwnProperty(sourceKey)) {
        const targetKey = additional_mapping_params[sourceKey];
        obj[targetKey] = obj[sourceKey];
      }
    });
    var newAttribution = JSON.stringify(obj);
    var storedAttribution = storage.getItem('attribution');
    if (rewriteAttribution(storedAttribution, newAttribution)) {
      storage.setItem('attribution', newAttribution);
      return newAttribution;
    }
    return storedAttribution;
  })();
</script>

